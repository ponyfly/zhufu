
.container {
  position: fixed;
  top:0;
  right:0;
  bottom: 0;
  left:0;
  background: url(data:image/png;base64,) repeat-y;
  background-size: 100%;
}
.container .themes {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-content: flex-start;
}
.container .themes .item-wrap {
  flex: 0 0 50%;
  display: flex;
  justify-content: center;
}
.container .themes .audio-card {

}
.container .themes .item-wrap .theme-item {
  width: 320rpx;
  height: 460rpx;
  margin: 30rpx 0 20rpx;
  border-radius: 12rpx;
  box-shadow: 6rpx 2rpx 10rpx 4rpx rgba(0,0,0,0.4);
}
.container .join-list {
  position: fixed;
  top:0;
  right:0;
  bottom: 120rpx;
  left:0;
}
.container .join-list .item-wrap {
 display: inline-block;
  vertical-align: top;
  width: 50%;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}
.container .join-list .item-wrap .join-item {
  width: 320rpx;
  height: 503rpx;
  margin:0 auto;
  box-shadow: 6rpx 2rpx 10rpx 4rpx rgba(0,0,0,0.4);
  background-color: #f7f2f0;
  border-radius: 8rpx;
  border-top-left-radius: 0;
  display: flex;
  flex-direction: column;
}
.container .join-list .item-wrap .join-item .content {
  width: 320rpx;
  height: 431rpx;
  position: relative;
}
.container .join-list .item-wrap .join-item .content .bg-video {
  position: absolute;
  width:100%;
  height:434rpx;
  box-shadow: 0 5rpx 10rpx rgba(0,0,0,.4);
  border-top-right-radius: 8rpx;
}
.container .join-list .item-wrap .join-item .content .audio-image {
  position: absolute;
  top:142rpx;
  left:0;
  right:0;
  margin:0 auto;
  width:263rpx;
  height:162rpx;
}
.container .join-list .item-wrap .join-item .content .audio-play {
  position: absolute;
  top:318rpx;
  left:0;
  right:0;
  margin:0 auto;
  width:76rpx;
  height:76rpx;
}

.container .join-list .item-wrap .join-item .content .video-image {
  position: absolute;
  top: 156rpx;
  left:16rpx;
  width:294rpx;
  height:265rpx;
}
.container .join-list .item-wrap .join-item .content .video-poster {
  position: absolute;
  top:107rpx;
  left:43rpx;
  width:217rpx;
  height:300rpx;
  transform:perspective(800px) rotateY(-18deg);
}

.container .join-list .item-wrap .join-item .content .card-head {
  position: absolute;
  top:186rpx;
  width:100%;
  height:26rpx;
}
.container .join-list .item-wrap .join-item .content .card-main {
  position: absolute;
  top:209rpx;
  width:100%;
  height:222rpx;
}

.container .join-list .item-wrap .join-item .content .bg-image {
  position: absolute;
  top:152rpx;
  left:18rpx;
  width:300rpx;
  height:280rpx;
}
.container .join-list .item-wrap .join-item .content .main-image {
  position:absolute;
  z-index:99;
  width:208rpx;
  height:156rpx;
  transform:rotateZ(20deg);
  background-color:rgba(0,0,0,.4);
  top:215rpx;
  left:51rpx;
}

.container .join-list .item-wrap .join-item .theme-title {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #5f3f08;
}
.container .tabbar {
  width: 100%;
  height: 215rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
}
.container .tabbar .tabbar-bg {
  width: 100%;
  height: 100%;
}
.container .tabbar .tabbar-content {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-top: 86rpx;
  box-sizing: border-box;
}
.container .tabbar .tabbar-content .home {
  width: 82rpx;
  height: 64rpx;
}
.container .tabbar .tabbar-content .home.checked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.container .tabbar .tabbar-content .home.unchecked {
  background: url(data:image/png;base64,) no-repeat;
  background-position: 2.5rpx 0;
  background-size: 76rpx 42rpx;
}
.container .tabbar .tabbar-content .myjoin {
  width: 149rpx;
  height: 64rpx;
}
.container .tabbar .tabbar-content .myjoin.checked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.container .tabbar .tabbar-content .myjoin.unchecked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 149rpx 43rpx;
}